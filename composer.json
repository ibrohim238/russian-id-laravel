{
  "name": "anper/russian-id-laravel",
  "type": "library",
  "description": "A set of useful Laravel validation rules for identifiers of Russian individuals and legal entities",
  "keywords": [
    "laravel",
    "rule",
    "bik",
    "rs",
    "ks",
    "oms",
    "emp",
    "orgn",
    "orgnip",
    "snils",
    "kpp",
    "inn",
    "check",
    "validate",
    "verify",
    "checksum",
    "расчётнный счёт",
    "корреспондентский счёт",
    "БИК",
    "ИНН",
    "ОМС",
    "ЕМП",
    "ОГРН",
    "ОГРНИП",
    "СНИЛС",
    "КПП"
  ],
  "homepage": "https://github.com/perevoshchikov/russian-id-laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Anton Perevoshchikov",
      "email": "anper3.5@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3||^8.0",
    "anper/russian-id": "^2.1",
    "illuminate/contracts": "^8.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "illuminate/validation": "^8.0",
    "orchestra/testbench": "^6.0",
    "phpstan/phpstan": "^0.12.99",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "Anper\\RussianId\\Laravel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Anper\\RussianId\\Laravel\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "stan": "phpstan analyse",
    "check-style": "phpcs",
    "fix-style": "phpcbf"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Anper\\RussianId\\Laravel\\RussianIdServiceProvider"
      ]
    }
  }
}
